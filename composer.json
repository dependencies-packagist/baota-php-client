{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "baota/client",
    "description": "BaoTa Panel SDK for PHP.",
    "keywords": [
        "BaoTa PHP SDK",
        "BaoTa PHP Client"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/baota-php-client",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.2.0"
    },
    "autoload": {
        "psr-4": {
            "BaoTa\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
